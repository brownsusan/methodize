<li class="task-item">
	<input type="checkbox" <% if(completed){%>checked<%}%>/>
	<span><%= title %></span>
	<% if (typeof dueDate != 'undefined') { %><span><%= dueDate %></span> <% } %>
	<input class="task-item-id" type="hidden" value="<%= id %>" />
	<img src="/img/icon/icon_23427.png" class="task-item-delete" />
	<span class="round-color-rep" style="background : <%= categoryObject.color %>"></span>
</li>